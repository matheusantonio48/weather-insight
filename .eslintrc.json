{
  "extends": ["next/core-web-vitals", "plugin:@typescript-eslint/recommended", "next", "prettier", "plugin:@next/next/recommended"],
  "plugins": ["react", "@typescript-eslint", "prettier"],
  "rules": {
    "prettier/prettier": "warn",
    "no-console": "warn",
    "prefer-const": "off",
    "@next/next/no-img-element": "off",
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "off"
  },
  "parser": "@typescript-eslint/parser",
  "ignorePatterns": ["jest.config.js", "designSystem/"]
}
